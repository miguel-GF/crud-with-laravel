--CREATE PROJECT
laravel new 'name-proyect' CRUD or composer create-project

--START SERVER
php artisan serve

--CREATE DATABASE
--CONF. FILE .env

--CREATE MIGRATION TABLE/S
php artisan make:migration create_name-table_table

--MIGRATE
php artisan migrate

--CREATE MODEL
php artisan make:model name-model

--CREATE CONTROLLER
php artisan make:controller NameController

